<div class="tab-panel row" id="bibliography">
    <div class="sixteen columns source-bibliography">
        <h3>Bibliography <small>&Dagger; <em>denotes primary source study</em></small></h3>

        {% for entry in content.bibliography %}
            <p>
                {% if entry.primary_study %}
                <strong> &Dagger;
                {% endif %}
                {{ entry.prerendered | safe }}
                {% if entry.pages %}
                    pp. {{ entry.pages }}.
                {% endif %}
                {% if entry.primary_study %}
                </strong>
                {% endif %}
                {% if user.is_staff %}<sup><a href="/admin/diamm_data/bibliography/{{ entry.pk }}/change">Edit</a></sup>{% endif %}
            </p>
        {% endfor %}
    </div>
</div>
